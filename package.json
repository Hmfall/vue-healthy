{
  "name": "vue-healthy",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prettier": "npx prettier . --write",
    "test:unit": "vitest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue,cjs,mts}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,vue}": [
      "npm run lint:fix",
      "vitest related --run --environment=jsdom"
    ]
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@pinia/testing": "^0.1.3",
    "@types/node": "^20.12.4",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.2",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.4.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.7"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "chart.js": "^4.4.2",
    "dayjs": "^1.11.10",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.0",
    "vuetify": "^3.6.3"
  }
}
